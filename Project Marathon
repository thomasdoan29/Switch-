
age = 29
let x = true
let early = true
let late = false

switch(age, x){
  case (age > 18 && early):
    console.log("Your starting time is 9:30 a.m.")
    break;
  case (age > 18 && late ):
    console.log("Your starting time is 11:00 a.m.")
    break;
  case (age < 18):
    console.log("Your starting time is 12:30 a.m.")
    break;
}
if (age > 18 && x) {
  console.log(Math.floor(Math.random() * 1000)+1000);
} else {
  console.log(Math.floor(Math.random() * 1000));
}
